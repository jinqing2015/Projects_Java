package cn.schema.simplefactory;

public class CheesePizza extends Pizza{
	
	public CheesePizza(){
		this.name = "CheesePizza";
	}
	@Override
	public void prepare() {
		// TODO Auto-generated method stub
		System.out.println("CheesePizza prepare");
	}

	@Override
	public void bake() {
		// TODO Auto-generated method stub
		System.out.println("CheesePizza prepare");
	}

	@Override
	public void cut() {
		// TODO Auto-generated method stub
		System.out.println("CheesePizza prepare");
	}

	@Override
	public void box() {
		// TODO Auto-generated method stub
		System.out.println("CheesePizza prepare");
	}
	
}
