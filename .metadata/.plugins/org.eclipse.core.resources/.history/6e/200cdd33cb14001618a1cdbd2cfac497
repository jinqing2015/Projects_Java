package cn.bh.Test;

public class InvokeRun extends Thread{
	
	private int i;
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for( ; i < 100; i++){
			System.out.println(Thread.currentThread().getName() + " " + i);
		}
	}
	
	//
	public static void main(String args[]){
		
		for(int i = 0; i < 100; i++)
	}

	
}
